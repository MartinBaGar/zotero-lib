  shorttitle = {An {{Efficient Gaussian-Accelerated Molecular Dynamics}} ({{GaMD}}) {{Multilevel Enhanced Sampling Strategy}}},
  author = {C{\'e}lerse, Fr{\'e}d{\'e}ric and Inizan, Th{\'e}o Jaffrelot and Lagard{\`e}re, Louis and Adjoua, Olivier and Monmarch{\'e}, Pierre and Miao, Yinglong and Derat, Etienne and Piquemal, Jean-Philip},
