@article{villasecaGaiProteinSubunit2022,
  title = {G{$\alpha$}i Protein Subunit: {{A}} Step toward Understanding Its Non-Canonical Mechanisms},
  shorttitle = {G{$\alpha$}i Protein Subunit},
  author = {Villaseca, Soraya and Romero, Gabriel and Ruiz, Mar{\'i}a J. and P{\'e}rez, Carlos and Leal, Juan I. and Tovar, Lina M. and Torrej{\'o}n, Marcela},
