@article{Michalska2018,
  title = {Functional Plasticity of Antibacterial {{EndoU}} Toxins},
  author = {Michalska, Karolina and Quan Nhan, Dinh and Willett, Julia L. E. and Stols, Lucy M. and Eschenfeldt, William H. and Jones, Allison M. and Nguyen, Josephine Y. and Koskiniemi, Sanna and Low, David A. and Goulding, Celia W. and Joachimiak, Andrzej and Hayes, Christopher S.},
