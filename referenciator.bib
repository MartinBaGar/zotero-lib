@article{bannertSialylatedOGlycansSulfated2001,
  title = {Sialylated {{O-Glycans}} and {{Sulfated Tyrosines}} in the {{NH2-Terminal Domain}} of {{CC Chemokine Receptor}} 5 {{Contribute}} to {{High Affinity Binding}} of {{Chemokines}}},
  author = {Bannert, Norbert and Craig, Stewart and Farzan, Michael and Sogah, Dodzie and Santo, Niki Villanueva and Choe, Hyeryun and Sodroski, Joseph},
  year = {2001},
