@article{sullivanExtracellularMatrixRemodeling2018,
  title = {Extracellular Matrix Remodeling Regulates Glucose Metabolism through {{TXNIP}} Destabilization},
  author = {Sullivan, William J. and Mullen, Peter J. and Schmid, Ernst W. and Flores, Aimee A. and Momcilovic, Milica and Sharpley, Mark S. and Jelinek, David and Whiteley, Andrew E. and Maxwell, Matthew B. and Wilde, Blake R. and Banerjee, Utpal and Coller, Hilary A. and Shackelford, David B. and Braas, Daniel and Ayer, Donald E. and {de Aguiar Vallim}, Thomas Q. and Lowry, William E. and Christofk, Heather R.},
