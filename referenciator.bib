@article{parkProteasomeInhibitorinducedCleavage2017,
  title = {Proteasome Inhibitor-Induced Cleavage of {{HSP90}} Is Mediated by {{ROS}} Generation and Caspase 10-Activation in Human Leukemic Cells},
