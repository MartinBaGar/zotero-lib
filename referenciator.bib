@article{hayashiMultifacetedNatureABcrystallin2020,
  title = {The Multifaceted Nature of {{$\alpha$B-crystallin}}},
  author = {Hayashi, Junna and Carver, John A.},
  year = {2020},
  month = jul,
  journal = {Cell Stress and Chaperones},
  series = {Perspectives on {{Small HSPs}}},
  volume = {25},
  number = {4},
  pages = {639--654},
  issn = {1355-8145},
  doi = {10.1007/s12192-020-01098-w},
  urldate = {2024-10-04},
  abstract = {In vivo, small heat-shock proteins (sHsps) are key players in maintaining a healthy proteome. {$\alpha$}B-crystallin ({$\alpha$}B-c) or HspB5 is one of the most widespread and populous of the ten human sHsps. Intracellularly, {$\alpha$}B-c acts via its molecular chaperone action as the first line of defence in preventing target protein unfolding and aggregation under conditions of cellular stress. In this review, we explore how the structure of {$\alpha$}B-c confers its function and interactions within its oligomeric self, with other sHsps, and with aggregation-prone target proteins. Firstly, the interaction between the two highly conserved regions of {$\alpha$}B-c, the central {$\alpha$}-crystallin domain and the C-terminal IXI motif and how this regulates {$\alpha$}B-c chaperone activity are explored. Secondly, subunit exchange is rationalised as an integral structural and functional feature of {$\alpha$}B-c. Thirdly, it is argued that monomeric {$\alpha$}B-c may be its most chaperone-species active, but at the cost of increased hydrophobicity and instability. Fourthly, the reasons why hetero-oligomerisation of {$\alpha$}B-c with other sHsps is important in regulating cellular proteostasis are examined. Finally, the interaction of {$\alpha$}B-c with aggregation-prone, partially folded target proteins is discussed. Overall, this paper highlights the remarkably diverse capabilities of {$\alpha$}B-c as a caretaker of the cell.},
  keywords = {B-crystallin,Molecular chaperone,Oligomerisation,Protein aggregation,Protein unfolding,Proteostasis,Small heat-shock proteins},
  file = {C\:\\Users\\martb\\Documents\\zotero-system\\storage\\VKTRD9WV\\Hayashi et Carver - 2020 - The multifaceted nature of αB-crystallin.pdf;C\:\\Users\\martb\\Documents\\zotero-system\\storage\\UP6PVIRV\\S1355814523012348.html}
}

