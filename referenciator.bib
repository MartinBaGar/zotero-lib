@article{Maschietto2023,
  title = {{{MDiGest}}: {{A Python}} Package for Describing Allostery from Molecular Dynamics Simulations},
  shorttitle = {{{MDiGest}}},
  author = {Maschietto, Federica and Allen, Brandon and Kyro, Gregory W. and Batista, Victor S.},
  year = {2023},
