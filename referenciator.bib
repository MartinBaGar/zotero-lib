@article{borrelPockDrugModelPredicting2015,
  title = {{{PockDrug}}: {{A Model}} for {{Predicting Pocket Druggability That Overcomes Pocket Estimation Uncertainties}}},
  shorttitle = {{{PockDrug}}},
  author = {Borrel, Alexandre and Regad, Leslie and Xhaard, Henri and Petitjean, Michel and Camproux, Anne-Claude},
  year = {2015},
